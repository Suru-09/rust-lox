fun sumToN(n) {
  if (n < 2)
    return 1;
  else {
    return n + sumToN(n - 1);
  }
}

var nForSum = 10;
print "Sum from one to " + nForSum + " is: " + sumToN(nForSum);

fun fib(n) {
  if (n <= 1) {
    return 1;
  }
  return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 20; i = i + 1) {
  print fib(i);
}

